ge refresh\n    // (accessToken is empty when page first loads)\n    if (!accessToken) await refreshToken();\n    getUser();\n  }, []);\n\n  return (\n    <>\n      {!loading && (\n        <UserContext.Provider value={{ user, setUser }}>\n          <BrowserRouter>\n            <Routes />\n          </BrowserRouter>\n        </UserContext.Provider>\n      )}\n    </>\n  );\n}\n\nexport default App;\n","/usr/src/app/src/reportWebVitals.js",[],"/usr/src/app/src/utils/accessToken.js",[],"/usr/src/app/src/styles/theme.js",[],"/usr/src/app/src/Routes.js",[],"/usr/src/app/src/utils/refreshToken.js",["149","150","151"],"import axios from 'axios';\nimport jwtDecode from 'jwt-decode';\nimport { setAccessToken } from './accessToken';\n\nconst refreshToken = () => {\n  console.log('refreshing token');\n  return (\n    axios\n      .post('/refresh_token', null, {\n        withCredentials: true,\n      })\n      .then((res) => {\n        console.log('refreshing token SUCCESS');\n        setAccessToken(res.data.accessToken);\n        // when access token expires, request a refresh token again\n        const { exp } = jwtDecode(res.data.accessToken);\n        const refreshIn = new Date(exp * 1000) - Date.now();\n        setTimeout(() => {\n          refreshToken();\n        }, refreshIn);\n      })\n      // eslint-disable-next-line no-unused-vars\n      .catch((err) => {\n        console.log('refreshing token ERROR');\n        setAccessToken(null);\n      })\n  );\n};\n\nexport default refreshToken;\n","/usr/src/app/src/components/UserContext.js",[],"/usr/src/app/src/styles/colors.js",[],"/usr/src/app/src/components/ProtectedRoute.js",[],"/usr/src/app/src/components/UnprotectedRoute.js",[],"/usr/src/app/src/store.js",[],"/usr/src/app/src/pages/Emprendimientos.js",[],"/usr/src/app/src/pages/Home.js",[],"/usr/src/app/src/pages/Login.js",[],"/usr/src/app/src/pages/Signup.js",[],"/usr/src/app/src/components/Nav/Nav.js",[],"/usr/src/app/src/components/EmprendimientoForm/EmprendimientoF